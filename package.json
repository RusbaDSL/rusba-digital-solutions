{
  "name": "rusba-digital-solutions",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../server && npm install",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd server && npm run dev",
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
